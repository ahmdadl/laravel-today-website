{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "artesaos/seotools": "^0.20.0",
        "cviebrock/eloquent-sluggable": "^8.0",
        "encore/laravel-admin": "^1.8",
        "enlightn/enlightn": "^1.4",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "hedronium/spaceless-blade": "^3.0",
        "jxlwqq/material-ui": "^1.0",
        "laravel-admin-ext/backup": "^1.1",
        "laravel-admin-ext/helpers": "1.*",
        "laravel-admin-ext/log-viewer": "^1.0",
        "laravel-admin-ext/scheduling": "^1.1",
        "laravel/framework": "^8.12",
        "laravel/ui": "^3.2",
        "spatie/laravel-backup": "^7.2",
        "tobiasdierich/gauge": "^0.0.5",
        "weidner/goutte": "^2.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.9",
        "beyondcode/laravel-websockets": "^1.9",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "fruitcake/laravel-telescope-toolbar": "^1.3",
        "laravel/breeze": "^1.0",
        "laravel/sail": "^1.0.1",
        "laravel/telescope": "^4.4",
        "laravel/tinker": "^2.5",
        "laravelium/sitemap": "^8.0",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3",
        "predis/predis": "^1.1",
        "sven/artisan-view": "^3.4"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "md": [
            "@php artisan ide-helper:models --nowrite"
        ],
        "md-pre": [
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta",
            "@php artisan ide-helper:models --nowrite"
        ]
    },
    "repositories": {
        "hooks": {
            "type": "composer",
            "url": "https://larapack.io"
        }
    }
}
